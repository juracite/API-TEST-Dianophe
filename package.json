{
  "name": "api-dianophe",
  "version": "1.0.0",
  "description": "Unofficial API from dianophe.fr",
  "main": "server.js",
  "scripts": {
    "test": "mocha -R spec test/test.js",
    "nm": "nodemon -e js,html,json,css server.js",
    "start": "node ./bin/www",
    "debug": "nodemon --inspect",
    "serv-doc": "docsify serve docs --port 1400",
    "deploy": "now -p"
  },
  "files": ["docs"],
  "docsify": {
    "config": {
      "basePath": "https://docsify.js.org/",
      "loadSidebar": true,
      "loadNavbar": true,
      "coverpage": true,
      "name": "docsify"
    }
  },
  "author": "Yann LEVEILLE-MENEZ @ Dianophe",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "cors": "^2.8.4",
    "docsify-cli": "^4.2.1",
    "now": "^10.2.1",
    "supertest": "^3.0.0"
  }
}
